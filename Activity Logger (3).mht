@charset "utf-8";

body { font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Helvetica, Arial, sans-serif; display: flex; justify-content: center; align-items: center; flex-direction: column; padding: 50px 20px; background-color: rgb(244, 244, 249); color: rgb(51, 51, 51); }

.button-container { display: flex; gap: 15px; margin-bottom: 20px; }

.clickBox { width: 200px; height: 60px; color: white; display: flex; justify-content: center; align-items: center; cursor: pointer; border-radius: 8px; font-size: 18px; text-align: center; user-select: none; box-shadow: rgba(0, 0, 0, 0.1) 0px 4px 6px; transition: background-color 0.2s, transform 0.2s; }

.clickBox:active { transform: scale(0.98); }

#exerciseButton { background-color: rgb(40, 167, 69); }

#exerciseButton:hover { background-color: rgb(33, 136, 56); }

#stellaButton { background-color: rgb(220, 53, 69); }

#stellaButton:hover { background-color: rgb(200, 35, 51); }

#calculateButton { background-color: rgb(255, 193, 7); color: rgb(33, 37, 41); font-weight: bold; margin-top: 20px; }

#calculateButton:hover { background-color: rgb(224, 168, 0); }

#clearLogButton { background-color: rgb(108, 117, 125); margin-top: 20px; }

#clearLogButton:hover { background-color: rgb(90, 98, 104); }

#logContainer { border: 1px solid rgb(221, 221, 221); border-radius: 8px; width: 90%; max-width: 400px; min-height: 100px; background-color: white; padding: 15px; }

#downloadLink { display: none; margin-top: 20px; color: rgb(0, 123, 255); text-decoration: none; font-weight: bold; }

#downloadLink:hover { text-decoration: underline; }

#pointsResult { margin-top: 20px; font-size: 24px; font-weight: bold; color: rgb(0, 86, 179); }

#dailyPointsTableContainer { margin-top: 30px; width: 90%; max-width: 400px; border: 1px solid rgb(221, 221, 221); border-radius: 8px; background-color: white; padding: 15px; display: none; }

#dailyPointsTableContainer h3 { text-align: center; margin-top: 0px; color: rgb(0, 86, 179); }

#dailyPointsTable { width: 100%; border-collapse: collapse; margin-top: 10px; }

#dailyPointsTable th, #dailyPointsTable td { border: 1px solid rgb(238, 238, 238); padding: 8px; text-align: left; }

#dailyPointsTable th { background-color: rgb(242, 242, 242); font-weight: bold; }

#dailyPointsTable td { background-color: rgb(255, 255, 255); }

.toggle-container { margin-top: 20px; display: flex; align-items: center; gap: 10px; font-size: 16px; font-weight: bold; color: rgb(85, 85, 85); }

.switch { position: relative; display: inline-block; width: 45px; height: 24px; }

.switch input { opacity: 0; width: 0px; height: 0px; }

.slider { position: absolute; cursor: pointer; inset: 0px; background-color: rgb(204, 204, 204); transition: 0.4s; border-radius: 24px; }

.slider::before { position: absolute; content: ""; height: 16px; width: 16px; left: 4px; bottom: 4px; background-color: white; transition: 0.4s; border-radius: 50%; }

input:checked + .slider { background-color: rgb(33, 150, 243); }

input:focus + .slider { box-shadow: rgb(33, 150, 243) 0px 0px 1px; }

input:checked + .slider::before { transform: translateX(21px); }

.slider.round { border-radius: 24px; }

.slider.round::before { border-radius: 50%; }
